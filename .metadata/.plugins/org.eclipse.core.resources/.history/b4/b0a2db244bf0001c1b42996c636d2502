package com.varxyz.banking.domain;

public class CustomerService {
	String Customer;
	String CustomerBySSn;
	String AllCustomers;
	String NumOfCustomers;
	
	public void addCustomer(String Customer) {
		this.Customer = Customer;
	}
	
	public void getCustomerBySsn(String CustomerBySSn) {
		this.CustomerBySSn = CustomerBySSn;
	}
	
	public void getAllCustomers(String AllCustomers) {
		this.AllCustomers = AllCustomers;
	}
	
	public void getNumOfCustomers(String NumOfCustomers) {
		this.NumOfCustomers = NumOfCustomers;
	}
}
