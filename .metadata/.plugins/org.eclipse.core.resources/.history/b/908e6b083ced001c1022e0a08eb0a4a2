package com.varxyz.jv200.mod007;

public class Book {
	String book;
	private int serial = 0;
	
	int bookSave = 100;
	int bookSaveMax = (bookSave) * 2;
	
	public void addBook(String book, int serial) {
		book = this.book;
		serial = this.serial;
	}
	
	public int getBook() {
		return serial;
	}
	
	public void setBook(int serial) {
		serial = this.serial;
	}
}